  "description": "Model Context Protocol server for Portland Maps, enabling AI assistants to query property zoning, history, and restrictions",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "portlandmaps-mcp-server": "./build/index.js"
  },
  "scripts": {
